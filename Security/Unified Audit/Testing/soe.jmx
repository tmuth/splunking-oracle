            <stringProp name="script">import java.util.Random;

// pick a randomr number between 0 and the number of records (minus 1)
int idx = new Random().nextInt(${customer_id_#});
idx = idx + 1;
//log.info(&quot;rand test: &quot;+ idx);

//
// get the first_name, last_name and customer_id for the random index number and set them as variables
custIdNum = vars.get(&quot;customer_id_&quot;+idx);
vars.put(&quot;cust_id&quot;,custIdNum);

custFirst = vars.get(&quot;first_name_&quot;+idx);
vars.put(&quot;first_name&quot;,custFirst);

custLast = vars.get(&quot;last_name_&quot;+idx);
vars.put(&quot;last_name&quot;,custLast);

</stringProp>
